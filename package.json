{
  "name": "my-app",
  "main": "expo/AppEntry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "web:proxy": "npx --yes browser-sync start --proxy 'http://localhost:8081' --files 'App.tsx,components/**/*.tsx,components/**/*.ts,screens/**/*.tsx,screens/**/*.ts,providers/**/*.ts,providers/**/*.tsx,constants/**/*.ts,constants/**/*.tsx,store/**/*.ts,store/**/*.tsx,tamagui.config.ts' --no-ui --no-notify --reload-delay 300",
    "web:reload": "node scripts/dev-web-reload.js",
    "lint": "expo lint"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/stack": "^7.4.7",
    "@tamagui/config": "^1.132.20",
    "ethers": "^6.15.0",
    "expo": "~53.0.20",
    "expo-constants": "~17.1.7",
    "expo-linking": "~7.1.7",
    "expo-web-browser": "~14.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "~0.20.0",
    "tamagui": "^1.132.20"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "typescript": "~5.8.3"
  },
  "private": true
}
